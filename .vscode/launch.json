    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'pitunes_client'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=pitunes_client",
                    "--package=pitunes_client"
                ],
                "filter": {
                    "name": "pitunes_client",
                    "kind": "bin"
                }
            },
            "args": ["https://localhost:8080"],
            "cwd": "${workspaceFolder}/pitunes_client"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'pitunes_client'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=pitunes_client",
                    "--package=pitunes_client"
                ],
                "filter": {
                    "name": "pitunes_client",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}/pitunes_client"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'pitunes_server'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=pitunes_server",
                    "--package=pitunes_server"
                ],
                "filter": {
                    "name": "pitunes_server",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}/pitunes_server"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'pitunes_server'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=pitunes_server",
                    "--package=pitunes_server"
                ],
                "filter": {
                    "name": "pitunes_server",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}/pitunes_server"
        }
    ]
}